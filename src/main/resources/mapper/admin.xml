<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xf.demo.dao.IAdminDao">
    <select id="selectByNameandPwd" resultType="java.lang.Integer">
        select count(*) from admin where name=#{name} and pwd=#{pwd}
    </select>
</mapper>